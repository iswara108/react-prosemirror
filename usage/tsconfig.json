{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "target": "ES2020",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["ES2020", "dom"],
    "types": ["node"],
    "baseUrl": "/"
  }
}
